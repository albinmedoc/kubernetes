apiVersion: v1
kind: Secret
metadata:
  name: postgres-secrets
  namespace: postgres
  labels:
    app.kubernetes.io/name: postgres
    app.kubernetes.io/component: configuration
    app.kubernetes.io/managed-by: kubectl
stringData:
  POSTGRES_DB: postgres
  POSTGRES_USER: root
  POSTGRES_PASSWORD: <POSTGRES_ROOT_PASSWORD>
